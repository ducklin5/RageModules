{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Release",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${env:RACK_SDK}/Rack.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${env:RACK_SDK}",
      "envFile": "${workspaceFolder}/config.env",
      "preLaunchTask": "Build application and add to Rack plugin folder",
      "externalConsole": false,
      "console":"internalConsole",
    },
    {
      "name": "Launch Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${env:RACK_SDK}/Rack.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${env:RACK_SDK}",
      "envFile": "${workspaceFolder}/config.env",
      "MIMode": "gdb",
      "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
      "preLaunchTask": "Build application without stripping symbols and add to Rack plugin folder",
      "externalConsole": false,
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    },
    {
      "name": "Launch Debug Dev",
      "type": "cppdbg",
      "request": "launch",
      "program": "${env:RACK_SDK}/Rack.exe",
      "args": ["-d"],
      "stopAtEntry": false,
      "cwd": "${env:RACK_SDK}",
      "envFile": "${workspaceFolder}/config.env",
      "MIMode": "gdb",
      "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
      "preLaunchTask": "Build application without stripping symbols and add to Rack plugin folder",
      "externalConsole": false,
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    }
  ]
}

